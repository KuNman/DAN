{% extends "base.html.twig" %}
    {% block body %}
        <table class="table table-striped table-hover">
            <thead>
            <tr>
                <th>#</th>
                <th>Id</th>
                <th>Author</th>
                <th>Comments</th>
                <th>Likes</th>
                <th>Date</th>
            </tr>
            </thead>
            <tbody>
{{ dump(data) }}
            {% for key, id in data.pageid %}
                <tr>
                   <td>{{ key }}</td>
                   <td>{{ id }}</td>
                   <td>Adam</td>
                   <td>0</td>
                    {% endfor %}
                </tr>
            </tbody>
        </table>
    {% endblock %}